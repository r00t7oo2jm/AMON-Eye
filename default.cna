	$out .= "\cC[*]\o Downloads\n\n";
	$out .= " Name               Size    Received        Path\n";
	$out .= "\cE ----               ----    --------        ----\n";
	
	foreach $entry ($2) {
		($name, $path, $size, $rcvd) = values($entry, @("name", "path", "size", "rcvd"));
		$pct  = round((double($rcvd) / $size) * 100.0, 1) . '%';
		$size = format_size($size);
		$rcvd = format_size($rcvd);
		$rcvd = "$rcvd ( $+ $pct $+ )";

