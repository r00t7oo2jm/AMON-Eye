popup beacon {
	separator();
	separator();


item "AV/EDR Recon" {
			local ('$bid');
			# The beacon id ($1) is an array even if you only have one beacon selected
			foreach $bid ($1) {
				reg_enum($bid, false);
			}
		}

	item "EDR exact query" {
		query($1);
	}
	
		menu "Session Management" {
		item "Note" {
			# resolve the note attached to the first highlighted beacon
			local('$note');
			$note = beacon_info($1[0], "note");

			# prompt the user for a new note.
			prompt_text("Set Beacon Note:", $note, lambda({
				binput($bids, "note $1");
				beacon_note($bids, $1);
			}, $bids => $1));
	
		}


		item "Remove" {
			beacon_remove($1);
		}

		item "Sleep" { 
			prompt_text("How long should beacon sleep for (seconds jitter%)?", "60", lambda({
				local('$time $jitter');
				if ($1 ismatch "(\\d+) (\\d+)") {
					($time, $jitter) = matched();
					binput($bids, "sleep $1");
					bsleep($bids, $time, $jitter);
				}
				else if ($1 ismatch "(\\d+)") {
					($time) = matched();
					binput($bids, "sleep $1");
					bsleep($bids, $time);
				}
				else {
					show_message("I did not understand $1");
				}
			}, $bids => $1));
		}

		item "&Exit" { 
			binput($1, "exit");
			bexit($1);
		}
	}
	
	item "Console Beacon" {
		local('$bid');
		foreach $bid ($1) {
			openOrActivate($bid);
		}
	}
		separator();
		separator();
}



